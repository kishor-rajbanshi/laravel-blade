{
    "name": "kishor-rajbanshi/laravel-blade",
    "description": "Effortlessly create custom Blade files in Laravel with a single Artisan command.",
    "keywords": [
        "laravel",
        "blade",
        "artisan",
        "template",
        "view",
        "generator",
        "scaffolding",
        "web development",
        "php",
        "package",
        "kishor-rajbanshi"
    ],
    "support": {
        "issues": "https://github.com/kishor-rajbanshi/laravel-blade/issues"
    },
    "homepage": "https://github.com/kishor-rajbanshi/laravel-blade",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "KishorRajbanshi\\Blade\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kishor Rajbanshi",
            "email": "kishorrajbanshi00@gmail.com",
            "homepage": "https://www.kishor-rajbanshi.com.np",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "KishorRajbanshi\\Blade\\BladeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
